{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "title": "shield-icq-v1",
  "properties": {
    @TELEMETRY_APPLICATION_1_JSON@,
    @TELEMETRY_CLIENTID_1_JSON@,
    @TELEMETRY_CREATIONDATE_1_JSON@,
    @TELEMETRY_ENVIRONMENT_1_JSON@,
    @TELEMETRY_ID_1_JSON@,
    "payload": {
      "properties": {
        "reason": {
          "enum": [
            "progress",
            "final",
            "error"
          ],
          "type": "string"
        },
        "type": {
          "oneOf": [
            {
              "enum": [
                "wrong-status",
                "missing-performance",
                "request-aborted",
                "request-error"
              ],
              "type": "string"
            },
            {
              "maxLength": 0,
              "type": "string"
            }
          ]
        },
        "latency": {
          "type": "number"
        },
        "fileSize": {
          "type": "number"
        },
        "connType": {
          "enum": [
            "slow-2g",
            "2g",
            "3g",
            "4g",
            "unknown"
          ],
          "type": "string"
        },
        "goodput": {
          "type": "array",
          "items": {
            "type": "array",
            "items": [
              {
                "type": "number"
              },
              {
                "type": "number"
              }
            ],
            "additionalItems": false
          }
        }
      },
      "required": [
        "reason",
        "type",
        "latency",
        "fileSize",
        "connType",
        "goodput"
      ],
      "type": "object"
    },
    "type": {
      "enum": [
        "shield-icq-v1"
      ],
      "type": "string"
    },
    "version": {
      "maximum": 4,
      "minimum": 4,
      "type": "number"
    }
  },
  "required": [
    "application",
    "clientId",
    "creationDate",
    "environment",
    "id",
    "payload",
    "type",
    "version"
  ]
}
